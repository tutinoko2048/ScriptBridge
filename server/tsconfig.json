{
  "compilerOptions": {
      "target": "es2022",
      "module": "Node16",
      "moduleResolution": "Node16",
      "alwaysStrict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitThis": true,
      "declaration": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "outDir": "dist",
      "noUncheckedIndexedAccess": true
  }
}